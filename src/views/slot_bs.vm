    #macro( game ,$game )
    #foreach($item in $game)
    <div class="slot-item">
        <img data-original="/static/images/game/${type}/$item.img">
        <div class="slot-item-btn">
            <a class="slot-item-btn-begin game-begin" data-id="$item.id" href="/begin" data-i18n="slot.8">立即游戏</a>
            #if($item.try)
            <a class="slot-item-btn-try" data-id="$item.id" href="/begin" data-i18n="slot.9">试玩游戏</a>
            #end
            <a class="slot-item-btn-down" href="/down">客户端下载</a>
        </div>
        <div class="slot-item-name">$item.name</div>
        <div class="slot-item-value" id="$item.id"></div>
    </div>
    #end
    #end

    #macro( gamelist ,$game, $active)
    <div class="slot-game-one #if($active)active#end" #if($active)data-load="1"#end>
        <div class="slot-game-main">
            <div class="slot-game-main-con clearfix">
                #game($game)
            </div>
        </div>
    </div>
    #end

