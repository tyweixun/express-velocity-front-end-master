    <link rel="stylesheet" href="/static/css/index.css"/>
</head>
<body>
    #parse('/common/old.vm')
    #parse('/common/header.vm')
    <div class="banner">
        <div class="cycle-slideshow"
             data-cycle-prev=".banner-op-left"
             data-cycle-next=".banner-op-right"
             data-cycle-slides=".banner-item"
             data-cycle-fx="scrollHorz" data-cycle-log="false"
             data-cycle-timeout="10000"
             data-cycle-pager=">.cycle-pager"
             data-cycle-pager-template="<span></span>"
                >V
            <div class="banner-item" style='background: url("/static/images/banner_index.jpg") no-repeat center;'></div>
            <div class="banner-item" style='background: url("/static/images/banner_index2.jpg") no-repeat center;'></div>
            <div class="banner-item" style='background: url("/static/images/banner_index3.jpg") no-repeat center;'></div>
            <div class="banner-item" style='background: url("/static/images/banner_index4.jpg") no-repeat center;'></div>
            <div class="banner-op">
                <div class="banner-op-item banner-op-left"></div>
                <div class="banner-op-item banner-op-right"></div>
            </div>
            <div class="cycle-pager"></div>
        </div>
    </div>
    <div class="prod">
