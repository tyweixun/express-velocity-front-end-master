                        <a href="#" data-i18n="index.16"></a>
                    </div>
                </div>
                <div class="notice-item notice-item-last">
                    <a href="#" class="notice-item-title" data-i18n="[prepend]index.17"><i></i></a>
                    <div class="notice-item-text" data-i18n="[html]index.18"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="index-bg"></div>
    </div>
    <div class="list-img"></div>
    <div class="list">
        <div class="container">
            <div class="list-con clearfix">
