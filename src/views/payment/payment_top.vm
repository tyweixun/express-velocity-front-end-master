<div id="account-info">
    <h3>
        <span class="hello">$disSidCust.hello，</span>
        <span class="membername">$disSidCust.username</span>
        <span class="tips">$disSidCust.tips</span>
    </h3>
    <div class="clearfix">
        <div class="account-safe fleft">
            <div class="illustration">
                <span class="safe-percent" id="info-complete" data-complete="$disSidCust.percentage">$disSidCust.percentage%</span>
                <input type="text" value="0" class="dial" style="display: none"/>
            </div>
            <div class="r">
                <div class="box-1"><span class="title" data-i18n="center:top.1"></span><span class="safe-level">$disSidCust.securityState</span></div>
                <div class="box-2">
                    #if($disSidCust.isBindName)
                    <div data-i18n="[data-title]center:top.2;[data-content]center:top.3;[title]center:top.4" class="#if($disSidCust.isBindName)logo-trueinfo-on #else logo-trueinfo#end" id="as-trueinfo" data-showtips="true" data-offset="10|0" href="/member/safe#name" target="_self"></div>
                    #else
                    <a data-i18n="[data-title]center:top.2;[data-content]center:top.3;[title]center:top.4" class="#if($disSidCust.isBindName)logo-trueinfo-on #else logo-trueinfo#end" id="as-trueinfo" data-showtips="true" data-offset="10|0" href="/member/safe#name" target="_self"></a>
                    #end


                    #if($disSidCust.isBindBank)
                    <div data-i18n="[data-title]center:top.5;[data-content]center:top.6;[title]center:top.7;[data-dialog]center:top.77" class="#if($disSidCust.isBindBank)logo-bindcard-on #else logo-bindcard#end" data-showtips="true" data-offset="10|0" href="/payment/withdraw"></div>
                    #else
                    <a data-i18n="[data-title]center:top.5;[data-content]center:top.6;[title]center:top.7;[data-dialog]center:top.77" class="#if($disSidCust.isBindBank)logo-bindcard-on #else logo-bindcard#end" id="as-bindcard" data-showtips="true" data-offset="10|0" href="/payment/withdraw"></a>
                    #end


                    #if($disSidCust.isBindPhone)
                    <div data-i18n="[data-title]center:top.8;[data-content]center:top.9;[title]center:top.10;[data-dialog]center:top.101" class="#if($disSidCust.isBindPhone)logo-bindphone-on #else logo-bindphone#end" id="as-bindphone" data-showtips="true" data-offset="10|0" href="#t-phone"></div>
                    #else
                    <a data-i18n="[data-title]center:top.8;[data-content]center:top.9;[title]center:top.10;[data-dialog]center:top.101" class="#if($disSidCust.isBindPhone)logo-bindphone-on #else logo-bindphone#end" id="as-bindphone" data-showtips="true" data-offset="10|0" href="#t-phone"></a>
                    #end


                    #if($disSidCust.isBindEmail)
                    <div data-i18n="[data-title]center:top.11;[data-content]center:top.12;[title]center:top.13;[data-dialog]center:top.14" class="#if($disSidCust.isBindEmail)logo-bindemail-on #else logo-bindemail#end mail-show" id="as-bindemail" data-showtips="true" data-offset="10|0" href="#t-mail"></div>
                    #else
                    <a data-i18n="[data-title]center:top.11;[data-content]center:top.12;[title]center:top.13;[data-dialog]center:top.14" class="#if($disSidCust.isBindEmail)logo-bindemail-on #else logo-bindemail#end mail-show" id="as-bindemail" data-showtips="true" data-offset="10|0" href="#t-mail"></a>
                    #end
                </div>
            </div>
        </div>
        <div class="line-2 fleft"></div>
        <div class="wallet fleft">
            <div class="clearfix">
                <span class="balance" id="balance">$disSidCust.accountStr</span>
                <span class="btn-refresh-2" data-type="curAccount" data-ele="#balance" data-i18n="[title]center:btn.1"></span>
            </div>
            <div>
                <span class="title" data-i18n="center:btn.2"></span>
            </div>
        </div>
        <div class="line-2 fleft"></div>
        <div class="funds-proportion fleft">
            <div>
                <span class="title" data-i18n="center:btn.3"></span>
            </div>
            <div class="funds-drawing">
                <div class="fd-tips">
                    <div class="detail" data-for="col-1">
                        <span class="name">中心钱包</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-2">
                        <span class="name">锁定钱包<span class="amount">0</span><span class="percent">占0%</span></span>
                        <span class="txt">此处一般为绑定个人信息后未进行提款操作的未到账彩金，以及正在处理中的提款金额</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-3">
                        <span class="name">体育钱包</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-4">
                        <span class="name">快乐彩</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-5">
                        <span class="name">金碧娱乐城</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-6">
                        <span class="name">GD娱乐城</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-7">
                        <span class="name">PT娱乐城</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-8">
                        <span class="name">AG娱乐城</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                    <div class="detail" data-for="col-9">
                        <span class="name">时时彩</span>
                        <span class="amount">0</span>
                        <span class="percent">占0%</span>
                        <span class="logo-sj6-b"></span>
                    </div>
                </div>
                <ul id="balances">
                    <li class="col-0">
                        <input type="hidden" value="">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                    <li class="col-1" data-amount="0">
                        <input type="hidden" value="main">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                    <li class="col-2" data-amount="0">
                        <input type="hidden" value="hold">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                    <li class="col-3" data-amount="0">
                        <input type="hidden" value="sportsbook">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>

                    <li class="col-4" data-amount="0">
                        <input type="hidden" value="keno">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                    <li class="col-5" data-amount="0">
                        <input type="hidden" value="grandsuite">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                    <li class="col-6" data-amount="0">
                        <input type="hidden" value="gdcasino">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li> <li class="col-7" data-amount="0">
                    <input type="hidden" value="ptcasino">
                    <div class="bar"></div>
                    <div class="h"></div>
                    <div class="s"></div>
                    <div class="f"></div>
                </li>
                    <li class="col-8" data-amount="0">
                        <input type="hidden" value="aggame">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                    <li class="col-9" data-amount="0">
                        <input type="hidden" value="lotto">
                        <div class="bar"></div>
                        <div class="h"></div>
                        <div class="s"></div>
                        <div class="f"></div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="funds-btns clearfix">
        <ul>
            <li #if($header=="deposit")class="active"#end><a data-i18n="center:btn.4" href="/payment/deposit" class="btn-gray-2 btn-deposit"></a><span class="logo-sj5"></span></li>
            <li #if($header=="withdraw")class="active"#end><a data-i18n="center:btn.5" href="/payment/withdraw" class="btn-gray-2 btn-withdraw"></a><span class="logo-sj5"></span></li>
            <li #if($header=="transfer")class="active"#end><a data-i18n="center:btn.6" href="/payment/transfer" class="btn-gray-2 btn-transfer"></a><span class="logo-sj5"></span></li>
            <li #if($header=="statement")class="active"#end><a data-i18n="center:btn.7" href="/payment/statement" class="btn-gray-2 btn-transactionlog"></a><span class="logo-sj5"></span></li>
        </ul>
    </div>
    <div class="trans_sd"></div>
</div>